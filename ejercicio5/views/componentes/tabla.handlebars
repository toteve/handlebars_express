{{!-- plantilla de tabla.handlebars para mostrar los usuarios enviados en la ruta /usuarios --}}

<table class="table">

    <thead>
        <th>Nombre</th>
        <th>Dirección</th>
        <th>Teléfono</th>
    </thead>

    <tbody>
        {{#each usuarios}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.direccion}}</td>
            <td>{{this.telefono}}</td>
        </tr>
        {{/each}}
    </tbody>

</table>